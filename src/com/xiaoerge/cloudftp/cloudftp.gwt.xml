<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="cloudftp">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.logging.Logging'/>

    <entry-point class='com.xiaoerge.cloudftp.client.CloudFTP'/>

    <servlet path='/authservice' class='com.xiaoerge.cloudftp.server.AuthServiceImpl'/>
    <servlet path="/PublicService" class="com.xiaoerge.cloudftp.server.PublicServiceImpl"/>

    <source path='client'/>
    <source path='shared'/>

</module>